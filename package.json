{
  "name": "puppeteer-typescript-boilerplate",
  "version": "0.1.0",
  "type": "module",
  "description": "A boilerplate for Puppeteer + TypeScript",
  "repository": "https://github.com/sosmii/puppeteer-typescript-boilerplate",
  "author": "sosmii",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn tsc src/index.ts",
    "run": "node dist/index.js",
    "lint": "tslint ./src/**/*.ts",
    "fix": "tslint --fix ./src/**/*.ts",
    "start": "yarn ts-node --esm src/index.ts"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@types/node": "^10.14.13",
    "@types/puppeteer": "^1.12.4",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^4.4.0"
  },
  "dependencies": {
    "jimp": "^0.22.7",
    "loglevel": "^1.8.1",
    "ts-node-esm": "^0.0.6",
    "winston": "^3.8.2"
  }
}
